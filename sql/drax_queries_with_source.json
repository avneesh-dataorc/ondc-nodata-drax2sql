[
    {
      "Table_name": "order_measures",
      "Measures_Name": "Confirmed",
      "Sources": "Ordernhm",
      "Measures": "Sum(Ordernhm[no_key])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Delivered",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n    Sum(Ordernhm[no_key]),\n    Ordernhm[ONDC order_status] = \"Delivered\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Cancelled",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n    Sum(Ordernhm[no_key]),\n    Ordernhm[ONDC order_status] = \"Cancelled\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "In Process",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n    Sum(Ordernhm[no_key]),\n    Ordernhm[ONDC order_status] = \"In Process\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Part Delivered",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n    Sum(Ordernhm[no_key]),\n    FILTER(Ordernhm,Ordernhm[ONDC order_status] = \"Part Delivered\")\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Buyer cancel",
      "Sources": "Ordernhm, Cancellation code",
      "Measures": "CALCULATE(\n   [Cancelled],\n   Ordernhm[ONDC order_status]=\"Cancelled\",\n   RELATEDTABLE('Cancellation code'),'Cancellation code'[attributed to]=\"Buyer app\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Seller cancel",
      "Sources": "Ordernhm, Cancellation code",
      "Measures": "CALCULATE(\n   Sum(Ordernhm[no_key]),\n   Ordernhm[ONDC order_status]=\"Cancelled\",\n   RELATEDTABLE('Cancellation code'),'Cancellation code'[attributed to]= \"Seller app\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Logistic cancel",
      "Sources": "Ordernhm, Cancellation code",
      "Measures": "CALCULATE(\n   Sum(Ordernhm[no_key]),\n   Ordernhm[ONDC order_status]=\"Cancelled\",\n   RELATEDTABLE('Cancellation code'),'Cancellation code'[attributed to]=\"Logistics Provider\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Fill Rate",
      "Sources": "",
      "Measures": "DIVIDE([Confirmed] - [Seller cancel],[Confirmed])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Seller NP",
      "Sources": "Ordernhm, Seller NP",
      "Measures": "DISTINCTCOUNT(Ordernhm[Seller NP])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Buyer NP",
      "Sources": "Ordernhm",
      "Measures": "DISTINCTCOUNT(Ordernhm[Buyer NP])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Delivery %",
      "Sources": "",
      "Measures": "DIVIDE(\n   [Delivered],\n   [Confirmed],BLANK()\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Order growth",
      "Sources": "Ordernhm",
      "Measures": "VAR lastdt =\nCALCULATE(\n   Sum(Ordernhm[no_key]),DATEADD(Ordernhm[Date],-1,DAY))\nVAR growth =\nDIVIDE((Sum(Ordernhm[no_key])-lastdt),lastdt)\nRETURN growth"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Active seller",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   DISTINCTCOUNT(Ordernhm[provider key]),\n   DATESMTD(Ordernhm[Date])\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Active buyer",
      "Sources": "Ordernhm, dimdate",
      "Measures": "CALCULATE(\n   DISTINCTCOUNT(Ordernhm[Buyer NP]),\n   DATESMTD(dimdate[Date])\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Seller Cancel %",
      "Sources": "",
      "Measures": "DIVIDE([Seller cancel],[Confirmed]\n,\"-\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Cancellation code cal",
      "Sources": "Ordernhm",
      "Measures": "COUNT(Ordernhm[cancellation_code])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Cancel code share",
      "Sources": "Cancellation code",
      "Measures": "DIVIDE([Cancellation code cal],[Confirmed all])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Cities with Active Sellers (MTD)",
      "Sources": "Ordernhm, dimdate",
      "Measures": "CALCULATE(\n   DISTINCTCOUNT(Ordernhm[Seller City]),\n   DATESMTD(dimdate[Date])\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Active Delivery Cities (MTD)",
      "Sources": "Ordernhm, dimdate",
      "Measures": "CALCULATE(\n   DISTINCTCOUNT(Ordernhm[Delivery City]),\n   DATESMTD(dimdate[Date])\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Tat breach %",
      "Sources": "Ordernhm",
      "Measures": "var buy_can_breach = CALCULATE([Buyer cancel],FILTER(Ordernhm,Ordernhm[Breach]=0))\nvar base = [Confirmed]-[Seller cancel]-[Logistic cancel] - buy_can_breach\nreturn\nDivide([Tat breach],base,BLANK())"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Tat breach",
      "Sources": "Ordernhm",
      "Measures": "SUMX(Ordernhm,Ordernhm[Breach])"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Promised TAT breach",
      "Sources": "Ordernhm",
      "Measures": "VAR Breach = CALCULATE(SUMX(Ordernhm,IF(Ordernhm[ONDC order_status] = \"Delivered\" && Ordernhm[Updated at]>Ordernhm[Promised time] || Ordernhm[ONDC order_status] = \"Cancelled\" && Ordernhm[Updated at]>Ordernhm[Promised time] || Ordernhm[ONDC order_status] = \"In Process\" && TODAY()>Ordernhm[Promised time],1,0)))\nReturn\nIF(Breach = 1, \"Yes\",\"No\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Breach (mins)",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(AVERAGEX(Ordernhm,\nIF(Ordernhm[ONDC order_status] = \"Delivered\", DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],SECOND)/60,\nIF(Ordernhm[ONDC order_status] = \"Cancelled\", DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],SECOND)/60,\nIF(Ordernhm[ONDC order_status] = \"In Process\", DATEDIFF(Ordernhm[Promised time],TODAY(),SECOND)/60,\nBLANK())))))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Breach (hrs)",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(AVERAGEX(Ordernhm,\nIF(Ordernhm[ONDC order_status] = \"Delivered\", DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],SECOND)/3600,\nIF(Ordernhm[ONDC order_status] = \"Cancelled\", DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],SECOND)/3600,\nIF(Ordernhm[ONDC order_status] = \"In Process\", DATEDIFF(Ordernhm[Promised time],TODAY(),SECOND)/3600,\nBLANK())))))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Breach (days)",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(AVERAGEX(Ordernhm,\nIF(Ordernhm[ONDC order_status] = \"Delivered\", DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],SECOND)/86400,\nIF(Ordernhm[ONDC order_status] = \"Cancelled\", DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],SECOND)/86400,\nIF(Ordernhm[ONDC order_status] = \"In Process\", DATEDIFF(Ordernhm[Promised time],TODAY(),SECOND)/86400,\nBLANK())))))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Provider id (MTD)",
      "Sources": "Ordernhm, dimdate",
      "Measures": "CALCULATE(\n   DISTINCTCOUNT('Ordernhm'[provider key]),\n   DATESMTD(dimdate[Date])\n)"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Average Retail OPD",
      "Sources": "Ordernhm",
      "Measures": "DIVIDE([Confirmed],DISTINCTCOUNT(Ordernhm[Date]))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Inter NP",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(Sum(Ordernhm[no_key]),Ordernhm[Order type]=\"Inter NP\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Intra NP",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(Sum(Ordernhm[no_key]),Ordernhm[Order type]=\"Intra NP\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Social Sellers on a day",
      "Sources": "Ordernhm, Social",
      "Measures": "CALCULATE(DISTINCTCOUNT(Ordernhm[provider key]),Ordernhm[Sector]=\"Social\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "G sheet",
      "Sources": "Social, Seller NP",
      "Measures": "CALCULATE(DISTINCTCOUNT(Social[Key3]),ALLSELECTED(Social[Seller NP (Provided by Expansion)]))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Logistic Cancel %",
      "Sources": "",
      "Measures": "DIVIDE([Logistic cancel],[Confirmed],\"-\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Social Cred Orders",
      "Sources": "Social, cred",
      "Measures": "CALCULATE([Confirmed],cred[cred_code] = \"Social Sector\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Social Orders Current",
      "Sources": "Ordernhm, Social",
      "Measures": "CALCULATE([Confirmed],Ordernhm[Sector] = \"Social\")"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Difference in Social Orders",
      "Sources": "Social, cred",
      "Measures": "[Social Orders Current] - [Social Cred Orders]"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Delivered with TAT",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],FILTER(Ordernhm,Ordernhm[Breach]=0))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Delivered beyond TAT",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],FILTER(Ordernhm,Ordernhm[Breach]=1))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "Delivered Breached only",
      "Sources": "Ordernhm",
      "Measures": "var a=\nCALCULATE(sum(Ordernhm[no_key]), Ordernhm[ONDC order_status] = \"Delivered\" && Ordernhm[Breach]=1)\nvar del=\nCALCULATE(sum(Ordernhm[no_key]), Ordernhm[ONDC order_status] = \"Delivered\" )\nvar buy_can = CALCULATE([Buyer cancel],Ordernhm[Shipped at]<>BLANK())\nvar log_can = CALCULATE([Logistic cancel],Ordernhm[Shipped at]<>BLANK())\nvar inproc = CALCULATE([In Process],Ordernhm[Shipped at]<>BLANK())\nReturn\nDIVIDE(a,(del+buy_can+log_can+inproc))"
    },
    {
      "Table_name": "order_measures",
      "Measures_Name": "DBO",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   SUM(Ordernhm[no_key]),\n   FILTER(\n       Ordernhm,\n       Ordernhm[ONDC order_status] = \"Delivered\"\n           && Ordernhm[Updated at] <= Ordernhm[Promised time]\n           && NOT(ISBLANK(Ordernhm[Updated at]))\n   )\n)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Non F&B days diff",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(AVERAGEX(FILTER(Ordernhm,Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category] <> \"F&B\"),DATEDIFF(Ordernhm[Created at],Ordernhm[Updated at],DAY))\n)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Same Day",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   SUM(Ordernhm[no_key]),\n    Ordernhm[day_diff] = 0 && Ordernhm[ONDC order_status]=\"Delivered\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Next Day",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   COUNTROWS(Ordernhm),\n   Ordernhm[day_diff] = 1 && Ordernhm[ONDC order_status]=\"Delivered\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Day + 2",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   COUNTROWS(Ordernhm),\n   Ordernhm[day_diff] = 2 && Ordernhm[ONDC order_status]=\"Delivered\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Confirmed all",
      "Sources": "Ordernhm, dimdate, Seller NP",
      "Measures": "CALCULATE(\n   COUNTROWS(Ordernhm),\n   ALLEXCEPT(Ordernhm,Ordernhm[Category],Ordernhm[ONDC order_status],'Ordernhm'[Delivery Type],'Seller NP'[seller_np],dimdate[Date])\n)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "More than 2 Days",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   COUNTROWS(Ordernhm),\n   Ordernhm[day_diff] > 2 && Ordernhm[ONDC order_status]=\"Delivered\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Same Day %",
      "Sources": "",
      "Measures": "DIVIDE([Same Day],[Delivered],\"-\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Next Day %",
      "Sources": "",
      "Measures": "DIVIDE([Next Day],[Delivered],\"-\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Day + 2 %",
      "Sources": "",
      "Measures": "DIVIDE([Day + 2],[Delivered],\"-\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "More than 2 Day %",
      "Sources": "",
      "Measures": "DIVIDE([More than 2 Days],[Delivered],\"-\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "F&B mins diff",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(AVERAGEX(Ordernhm,\n   DATEDIFF([Created at],[Updated at],MINUTE)),\n   FILTER(Ordernhm,Ordernhm[Category] = \"F&B\"),\n   FILTER(Ordernhm,[ONDC order_status]=\"Delivered\")\n)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "30 - 45 mins",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   Sum(Ordernhm[no_key]),\n   Ordernhm[min_diff] > 30 &&  Ordernhm[min_diff]<=45,Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category]=\"F&B\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "45 - 60 mins",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   Sum(Ordernhm[no_key]),\n   Ordernhm[min_diff] > 45 &&  Ordernhm[min_diff]<=60 && Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category]=\"F&B\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "60 - 120 mins",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   COUNTROWS(Ordernhm),\n   Ordernhm[min_diff] > 60 &&  Ordernhm[min_diff]<=120 && Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category]=\"F&B\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Above 120 mins",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   COUNTROWS(Ordernhm),\n   Ordernhm[min_diff] >120 && Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category]=\"F&B\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "45 - 60 mins %",
      "Sources": "",
      "Measures": "DIVIDE([45 - 60 mins],[Delivered],BLANK())"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "60 - 120 mins %",
      "Sources": "",
      "Measures": "DIVIDE ([60 - 120 mins],[Delivered],BLANK())"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "30 - 45 mins %",
      "Sources": "",
      "Measures": "DIVIDE ([30 - 45 mins],[Delivered],BLANK())"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Above 120 mins %",
      "Sources": "",
      "Measures": "DIVIDE([Above 120 mins],[Delivered],BLANK())"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "On time deliveries",
      "Sources": "Ordernhm",
      "Measures": "SUM(Ordernhm[on-time-del])"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "On time delivered %",
      "Sources": "Ordernhm",
      "Measures": "var buy_can_breach = CALCULATE([Buyer cancel],Ordernhm[Breach]=0)\nvar base = [Confirmed]-[Seller cancel]-[Logistic cancel] - buy_can_breach\nreturn\nDivide([On time deliveries],base,BLANK())"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Average TAT (in mins)",
      "Sources": "Ordernhm",
      "Measures": "AVERAGEX(FILTER(Ordernhm,Ordernhm[ONDC order_status]=\"Delivered\"),[tat_time])"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Average TAT (in days)",
      "Sources": "Ordernhm",
      "Measures": "DIVIDE(AVERAGEX(FILTER(Ordernhm,Ordernhm[ONDC order_status]=\"Delivered\"),[tat_time]),1440)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Average TAT (in hrs)",
      "Sources": "",
      "Measures": "[Average TAT (in mins)]/60"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Avg Delivery (hrs)",
      "Sources": "Ordernhm",
      "Measures": "DIVIDE(CALCULATE(AVERAGEX(FILTER(Ordernhm,Ordernhm[ONDC order_status]=\"Delivered\" && [min_diff]>0),[min_diff])),60)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Avg Delivery (mins)",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(AVERAGEX(FILTER(Ordernhm,Ordernhm[ONDC order_status]=\"Delivered\" && [min_diff]>0),[min_diff]))"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Avg Delivery (days)",
      "Sources": "Ordernhm",
      "Measures": "DIVIDE(CALCULATE(AVERAGEX(FILTER(Ordernhm,Ordernhm[ONDC order_status]=\"Delivered\" && [min_diff]>0),[min_diff])),1440)"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "15 - 30 mins",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   Sum(Ordernhm[no_key]),\n   Ordernhm[min_diff] > 15 &&  Ordernhm[min_diff]<=30,Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category]=\"F&B\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Within 15 mins",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(\n   Sum(Ordernhm[no_key]),\n   Ordernhm[min_diff]<=15,Ordernhm[ONDC order_status]=\"Delivered\" && Ordernhm[Category]=\"F&B\")"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "15 - 30 mins %",
      "Sources": "",
      "Measures": "DIVIDE([15 - 30 mins],[Delivered],BLANK())"
    },
    {
      "Table_name": "Delivery ageing measures",
      "Measures_Name": "Within 15 mins %",
      "Sources": "",
      "Measures": "DIVIDE([Within 15 mins],[Delivered],BLANK())"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT+1",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],\n   FILTER(Ordernhm,Ordernhm[tat_diff_days] = 1))"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT + 2",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],\n   FILTER(Ordernhm,Ordernhm[tat_diff_days] = 2))"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT + 3",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],\n   FILTER(Ordernhm,Ordernhm[tat_diff_days] = 3))"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT + 4",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],\n   FILTER(Ordernhm,Ordernhm[tat_diff_days] = 4))"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT > 4",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],\n   FILTER(Ordernhm,Ordernhm[tat_diff_days] > 4))"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT+1 %",
      "Sources": "",
      "Measures": "DIVIDE([TAT+1],[Confirmed],BLANK())"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT + 2 %",
      "Sources": "",
      "Measures": "DIVIDE([TAT + 2],[Confirmed],BLANK())"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT + 3 %",
      "Sources": "",
      "Measures": "DIVIDE([TAT + 3],[Confirmed],BLANK())"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT + 4 %",
      "Sources": "",
      "Measures": "DIVIDE([TAT + 4],[Confirmed],BLANK())"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT > 4 %",
      "Sources": "",
      "Measures": "DIVIDE([TAT > 4],[Confirmed],BLANK())"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT < 1",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE(SUMX(\n   FILTER(Ordernhm,[ONDC order_status]=\"Delivered\"),\n   IF(AND(DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],HOUR)< 24,\n   DATEDIFF(Ordernhm[Promised time],Ordernhm[Updated at],HOUR)>0),1,0)))"
    },
    {
      "Table_name": "TAT measures",
      "Measures_Name": "TAT 0-6 hrs",
      "Sources": "Ordernhm",
      "Measures": "CALCULATE([Delivered],FILTER(Ordernhm,\n   Ordernhm[tat_dif]>0 && Ordernhm[tat_dif]<=21600))"
    },
    {
      "Table_name": "TAT measures",
